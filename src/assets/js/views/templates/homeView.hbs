<div class="row">
    <div class="col-md-12">
        <h4><span class="user-img"></span> Users List</h4>
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Phone</th>
                    <th>Email ID</th>
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <td class="user_name"><span class="glyphicon glyphicon-user"></span>{{name}}</td>
                    <td>{{phone}}</td>
                    <td>{{email}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>